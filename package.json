{
  "name": "questionly",
  "version": "1.0.0",
  "description": "Generate a Microsoft Teams application.",
  "scripts": {
    "start": "node dist/server.js",
    "build": "gulp build",
    "debug": "gulp serve --debug",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@fluentui/react-northstar": "~0.49.0",
    "@microsoft/teams-js": "1.6.0",
    "applicationinsights": "^1.3.1",
    "axios": "^0.19.2",
    "botbuilder": "4.7.1",
    "botbuilder-dialogs": "4.7.1",
    "botbuilder-teams-messagingextensions": "1.6.0",
    "compression": "1.7.4",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "express": "4.16.4",
    "express-msteams-host": "1.6.2",
    "express-session": "1.15.6",
    "gulp-exec": "^5.0.0",
    "morgan": "1.9.1",
    "msteams-react-base-component": "^2.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/debug": "4.1.2",
    "@types/express": "4.16.0",
    "@types/express-session": "1.15.10",
    "@types/morgan": "1.7.35",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.11",
    "@types/enzyme-to-json": "^1.5.3",
    "autoprefixer": "^9.5.1",
    "browser-sync": "^2.26.5",
    "del": "^5.1.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "fancy-log": "1.3.2",
    "gulp-if": "^2.0.2",
    "gulp-inject": "5.0.2",
    "gulp-load-plugins": "^1.6.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-token-replace": "1.1.5",
    "gulp-zip": "4.2.0",
    "gulp": "4.0.2",
    "jest": "^24.5.0",
    "ngrok": "^3.1.1",
    "nodemon": "1.19.0",
    "plugin-error": "1.0.1",
    "plumber": "^0.4.8",
    "ts-loader": "7.0.1",
    "tslint-webpack-plugin": "^1.2.2",
    "tslint": "^5.11.0",
    "typescript": "3.3.4000",
    "vinyl": "2.2.0",
    "webpack": "4.26.0",
    "yargs": "15.3.1",
    "z-schema": "^4.2.2",
    "tslint-loader": "^3.6.0",
    "@types/react": "16.8.10",
    "file-loader": "1.1.11",
    "typestyle": "2.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "src/app/scripts/**/*.{js,jsx,ts,tsx}",
      "!src/app/scripts/client.ts",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
